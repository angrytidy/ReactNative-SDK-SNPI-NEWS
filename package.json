{
  "name": "snpiNews",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "open-studio": "open -a /Applications/Android ./android",
    "android-staging": "cat src/config/api-staging.js > src/config/api.js && react-native run-android",
    "android-production": "cat src/config/api-production.js > src/config/api.js && react-native run-android",
    "build-staging": "cat src/config/api-staging.js > src/config/api.js && cd android && ./gradlew assembleRelease",
    "build-production": "cat src/config/api-production.js > src/config/api.js && cd android && ./gradlew assembleRelease",
    "build-staging-store": "cat src/config/api-staging.js > src/config/api.js && cd android && ./gradlew bundleRelease",
    "build-production-store": "cat src/config/api-production.js > src/config/api.js && cd android && ./gradlew bundleRelease",
    "test-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@reduxjs/toolkit": "^2.8.2",
    "jetifier": "^2.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-devtools": "^6.1.5",
    "react-dom": "^19.1.0",
    "react-native": "0.80.1",
    "react-native-auto-height-image": "^3.2.4",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localization": "^2.3.2",
    "react-native-open-pdf": "^0.0.2",
    "react-native-popup-menu": "^0.18.0",
    "react-native-reanimated": "^4.0.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.15.0",
    "react-native-wonderpush": "^2.4.0",
    "react-native-worklets": "^0.4.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-promise": "^0.6.0",
    "validate.js": "^0.13.1"
